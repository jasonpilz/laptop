#!/bin/bash

set -eouv pipefail

cd ~ || true

mkdir -p .laptop && cd .laptop
curl -O https://raw.githubusercontent.com/jasonpilz/laptop/10.15-catalina/.laptop/laptop.install
curl -O https://raw.githubusercontent.com/jasonpilz/laptop/10.15-catalina/.laptop/Brewfile

/usr/bin/env bash ~/.laptop/laptop.install 2>&1 | tee ~/.laptop/laptop.log
